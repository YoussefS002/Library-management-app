<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.tp_bibliotheque.nouvelleEditionController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox>
        <children>
            <ComboBox fx:id="cbOeuvre" promptText="oeuvre" />
            <TextField fx:id="tfIsbn" promptText="isbn" />
            <TextField fx:id="tfEditeur" promptText="editeur" />
            <TextField fx:id="tfAnneeEdition" promptText="année d'édition" />
            <TextField fx:id="tfNbExemplaires" promptText="nombre d'exemplaires" />
            <Button mnemonicParsing="false" onAction="#ajouterEdition" text="Ajouter édition" />
        </children>
    </VBox>

</AnchorPane>
