<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.tp_bibliotheque.nouveauxExemplairesController"
            prefHeight="400.0" prefWidth="600.0"
            fx:id="mainContent">
    <VBox>
        <children>
            <ComboBox fx:id="cbOeuvre" promptText="oeuvre" onAction="#afficherIsbns"/>
            <ComboBox fx:id="cbIsbn" promptText="isbn" />
            <TextField fx:id="tfNbExemplaires" promptText="nombre d'exemplaires" />
            <Button mnemonicParsing="false" onAction="#ajouterExemplaires" text="Ajouter" />
            <Button mnemonicParsing="false" onAction="#retour" text="Annuler" />
        </children>
    </VBox>
</AnchorPane>